<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:android="http://schemas.android.com/apk/res/android"
            app:startDestination="@id/homeFragment">
	
	
	<fragment
			android:id="@+id/homeFragment"
			android:name="com.chethan.appuapplication.HomeFragment"
			android:label="HomeFragment">
		
		<action
				android:id="@+id/showTermsConditionFragment"
				app:destination="@id/termsConditionFragment"/>
		
		<action
				android:id="@+id/showPrivacyFragment"
				app:destination="@id/privacyFragment"/>
		
		
	</fragment>
	
	<fragment
			android:id="@+id/termsConditionFragment"
			android:name="com.chethan.appuapplication.TermsConditionFragment"
			android:label="termsConsitionFragment">
	
	</fragment>
	
	<fragment
			android:id="@+id/privacyFragment"
			android:name="com.chethan.appuapplication.PrivacyFragment"
			android:label="VenueDetailFragment">
	
	</fragment>


</navigation>